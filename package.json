{
  "name": "sample",
  "version": "0.0.1",
  "description": "React project boilerplate with webpack and ES6",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist",
    "devServer": "node ./server/dev.js",
    "deployServer": "node ./server/deploy.js",
    "dev": "npm run devServer",
    "libs": "cp -rf src/assets/libs dist/assets",
    "push": "rm -rf deploy/ & cp -rf dist ./deploy",
    "buildHtml": "node ./webpack/config.js && NODE_ENV=production webpack --progress --config webpack.production.html.js",
    "buildAssets": "NODE_ENV=production webpack -p --progress --config webpack.production.assets.js",
    "buildProduction": "npm run clean && npm run buildHtml && npm run buildAssets&& npm run libs && cp ./src/assets/images/favicon.ico dist/favicon.ico",
    "build": "npm run clean && NODE_ENV=production webpack -p --progress  --colors --config webpack.production.js && npm run libs && cp ./src/assets/images/favicon.ico dist/favicon.ico",
    "deploy": "npm run build && npm run deployServer",
    "wdeploy": "npm run clean&&set NODE_ENV=production&&webpack -p --progress  --colors --config webpack.production.js "
  },
  "repository": {
    "type": "git",
    "url": "https://git.systex.com/softmobie/fundrich-web.git"
  },
  "keywords": [
    "webpack",
    "boiler",
    "plate"
  ],
  "author": "Angus",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-check-es2015-constants": "^6.7.2",
    "babel-plugin-transform-es2015-arrow-functions": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^3.3.6",
    "css-loader": "^0.23.1",
    "node-sass": "^3.4.2",
    "react-dom": "^15.0.1",
    "react-tap-event-plugin": "^1.0.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  },
  "dependencies": {
    "axios": "^0.11.0",
    "babel": "^6.5.2",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "css-loader": "^0.23.1",
    "ejs-loader": "^0.2.1",
    "esformatter-jsx": "^5.0.0",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.26.7",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.2.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^1.6.2",
    "i18n-webpack-plugin": "^0.2.7",
    "json-loader": "^0.5.4",
    "material-ui": "^0.14.4",
    "opn": "^4.0.1",
    "react": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.5.2",
    "static-render-webpack-plugin": "^0.1.2",
